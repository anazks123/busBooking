<!DOCTYPE html>
<html>
<head>
  <title>A&S Express</title>
</head>
<body style="background: #232526;  /* fallback for old browsers */
background: -webkit-linear-gradient(to right, #414345, #232526);  /* Chrome 10-25, Safari 5.1-6 */
background: linear-gradient(to right, #414345, #232526); /* W3C, IE 10+/ Edge, Firefox 16+, Chrome 26+, Opera 12+, Safari 7+ */
">
<center>
<h1 style="color:white">A&S Express<br></h1>
</center>
<center>
<table border="100">
    <tr>
        <td>
        <fieldset>
        <legend><h2 style="color:white;">Search Avaialable Panel</h2></legend>
		<form method="post">
     
        <table>
            <tr>
                <td>
                <h3 style="color: white;">Sourcec :</h3>
             
                  <select name="start" style="width: 200px;height: 40px">
                    <% for(var i=0; i < result.length; i++){ %>
                    <option value="<%= result[i].start %>"><%= result[i].start %></option>
                    <% } %>
                  </select>
              
                  </td>
                  <td><br><br>
                &ensp; To &ensp;
                </td>
                <td>
                <h3 style="color:white">Destination :</h3>
               
                  <select name="end" style="width: 200px;height: 40px">
                    <% for(var i=0; i < result.length; i++){ %>
                    <option value="<%= result[i].end %>"><%= result[i].end %></option>
                    <% } %>
                  </select>
                
                </td>
              </tr>
              </table>
              
                  <br><br><br><center>
				  <input type="submit" name="submit"style="height: 40px; width:90px;background-color:yellow" value="Search">&ensp;
				  </center>
     
                  </form>

              </fieldset>
            </td>
          </tr>
          </table>
        </center>
    </body>
    </html>

